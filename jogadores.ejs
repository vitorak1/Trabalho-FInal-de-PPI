<h1>Jogadores</h1><% if(erro){ %><p><%= erro %></p><% } %><form method='post'><input name='nome'/><input name='nick'/><select name='funcao'><option>top</option><option>jungle</option><option>mid</option><option>atirador</option><option>suporte</option></select><input name='elo'/><input name='genero'/><select name='equipe'><% equipes.forEach(eq=>{ %><option><%= eq.nome %></option><% }) %></select><button>Salvar</button></form><ul><% jogadores.forEach(j=>{ %><li><%= j.nome %> - <%= j.equipe %></li><% }) %></ul>